{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://192.168.86.182:9400"
        }
      },
      {
        "Name": "Console"
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "ApplicationName": "appName API"
    }
  },
  "Authorization": {
    "Issuer": "XXXX",
    "Audience": "XXXX",
    "TokenExpires": 60
  },
  "ServiceBusSettings": {
    "ServerName": "192.168.86.181",
    "ListenQueueName": "MibComplete",
    "SubmitQueueName": "SubmitMib",
    "NumberOfRetries": "3",
    "RetryInterval": "5000"
  },
  "Management": {
    "Endpoints": {
      "Actuator": {
        "Exposure": {
          "Include": ["*"]
        }
      },
      "Metrics": {
        "Enabled": true
      }
    },
    "tracing": {
      "Name": "appName",
      "alwaysSample": true,
      "useShortTraceIds": false,
      "exporter": {
        "zipkin": {
          "endpoint": "http://192.168.86.183:9411/api/v2/spans",
          "validateCertificates": false,
          "useShortTraceIds ": false,
          "ServiceName": "appName"
        }
      }
    }
  }
}
